<VirtualHost *:80>
ServerName orvd.com
DocumentRoot /var/www/orvd/

WSGIDaemonProcess app user=www-data group=www-data threads=5
WSGIScriptAlias / /var/www/orvd/orvd_server.wsgi

ErrorLog /dev/stderr
CustomLog /dev/stdout combined

<Directory /var/www/orvd>
WSGIProcessGroup app
WSGIApplicationGroup %{GLOBAL}
Order deny,allow
Require all granted
</Directory>
</VirtualHost>
